<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Manejo de Eventos Onload del DOM</title>
</head>
<body onload="entrar()">
    <h1 id="titulo">Manejo del DOM con Javascript Onload</h1><br>

    <div id="mostrar"></div>

    <script>
        function entrar(){
            // alert("Entrando a la pagina web");

            let texto = "";

            if(navigator.cookieEnabled){
                texto = "Las cookies estan habilitadas";
            }else{
                texto = "Las cookies estan inhabilitadas";
            }
            document.getElementById("mostrar").innerHTML = texto;
        }
    </script>
</body>
</html>